{
    "VLAN_MEMBER": {
{% for vrf_idx in range(1, 1+vrf_count) %}
{% set vid = src_base_vid + vrf_idx %}
{% set vid2 = dst_base_vid + vrf_idx %}
{% if op_code == 'add' %}
        "Vlan{{vid}}|{{ dut_port1 }}": {
            "tagging_mode": "tagged"
        },
        "Vlan{{vid2}}|{{ dut_port2 }}": {
            "tagging_mode": "tagged"
        }{{ ',' if not loop.last else '' }}
{% else %}
        "Vlan{{vid}}|{{ dut_port1 }}": null,
        "Vlan{{vid2}}|{{ dut_port2 }}": null{{ ',' if not loop.last else '' }}
{% endif %}
{% endfor %}
    }
}