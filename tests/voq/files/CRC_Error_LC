"port enable eth yes"
"port loopback eth mode=mac"
"log off; rm /tmp/esmode; log file=/tmp/esmode quiet=yes"
"echo bcm_port_force_forward_set(0,1,1,1);"
"log off"
"cint /tmp/esmode"
"tx 1001 psrc=1 data='ffffffffffff00000000000108007777777777777777777777777777777777777'"
"s IPS_DRAM_ONLY_QUEUE_TYPE -1"
"mod CGM_VOQ_SRAM_DRAM_ONLY_MODE 0 64  SRAM_DRAM_ONLY_MODE=2"
"for i=0,100 's SPB_FORCE_CRC_ERROR -1; sleep 1'"
