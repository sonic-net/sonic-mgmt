            "has_per_asic_scope": true,
            "auto_restart": "disabled",
            "high_mem_alert": "disabled"
        },
        "pmon": {
            "state": "enabled",
            "has_timer": false,
            "has_global_scope": true,
            "has_per_asic_scope": false,
            "auto_restart": "enabled",
            "high_mem_alert": "disabled"
        },
        "radv": {
