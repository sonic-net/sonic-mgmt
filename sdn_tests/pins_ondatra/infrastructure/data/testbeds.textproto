# proto-message: ondatra.Testbed

# DUT device with 20 ports.
duts {
  id: "DUT"
  ports {
    id: "port1"
  }
  ports {
    id: "port2"
  }
  ports {
    id: "port3"
  }
  ports {
    id: "port4"
  }
}

# CONTROL device with 20 ports.
duts {
  id: "CONTROL"
  ports {
    id: "port1"
  }
  ports {
    id: "port2"
  }
  ports {
    id: "port3"
  }
  ports {
    id: "port4"
  }
}

# Specify the links between DUT and CONTROL.

# Below link represents DUT:port1 mapped to CONTROL:port1
links {
  a: "DUT:port1"
  b: "CONTROL:port1"
}
# Below link represents DUT:port2 mapped to CONTROL:port2
links {
  a: "DUT:port2"
  b: "CONTROL:port2"
}
links {
  a: "DUT:port3"
  b: "CONTROL:port3"
}
links {
  a: "DUT:port4"
  b: "CONTROL:port4"
}
